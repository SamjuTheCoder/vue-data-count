{
  "name": "vue-data-count",
  "description": "A simple vue data count",
  "version": "0.1.0",
  "private": false,
  "repository": "https://github.com/SamjuTheCoder/vue-data-count",
  "bugs": "https://github.com/SamjuTheCoder/vue-data-count/issues",
  "keywords": ["data count", "vue", "javascript"],
  "license": "MIT",
  "files": [ "dist/"],
